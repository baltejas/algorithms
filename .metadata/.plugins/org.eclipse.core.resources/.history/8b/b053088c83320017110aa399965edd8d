package com.algorithms.logic;

public class Alogirthms {

	public static void main(String[] args) {
		int i = 2; // log(1) to base 10 = 0
		while(i > 0) {
			// Steps to sort n inputs: insertion sort - 8n*n
			// Steps to sort n inputs: merge sort - 64nlgn
			if((8 * i * i) > (64 * i * (Math.log10(i)))) {
				// 
				System.out.println(i);
				break;
			} else {
				i++;
			}
		}
	}
}
